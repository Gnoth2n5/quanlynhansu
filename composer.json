{
    "name": "vendor/quanlychamcong",
    "description": "Web application for managing employee timekeeping",
    "type": "project",
    "scripts": {
        "serve": "php -S localhost:1234 routes/web.php",
        "public": "php -S xxx.xx.xx.xxx:8000 routes/web.php -t public"
    },
    "require": {
        "phroute/phroute": "^2.2",
        "eftec/bladeone": "^4.13",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^11.30",
        "rakit/validation": "^1.4",
        "symfony/console": "^7.1",
        "php-flasher/php-flasher": "^2.0",
        "php-flasher/flasher-toastr": "^2.1",
        "php-flasher/flasher-sweetalert": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Pham Minh Thong"
        }
    ],
    "minimum-stability": "stable"
}
